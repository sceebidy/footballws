    <foaf:name>Paris FC</foaf:name>
    <foot:country>France</foot:country>
    <foot:stadium>Stade Jean-Bouin</foot:stadium>
    <foot:manager>Stéphane Gilli</foot:manager>
    <foot:formedYear>1969</foot:formedYear>
